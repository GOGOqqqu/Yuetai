.song {
  width: 100%;
  position: relative;
}
.song .c {
  width: 100%;
  height: 500rppx;
}
.song .head {
  width: 96%;
  padding: 40rpx 2%;
  height: 250rpx;
  display: flex;
  justify-content: space-between;
}
.song .head .head-left {
  width: 35%;
  height: 250rpx;
}
.song .head .head-left .img {
  width: 100%;
  height: 250rpx;
  border-radius: 5%;
}
.song .head .head-right {
  width: 62%;
  height: 250rpx;
  color: indianred;
}
.song .head .head-right .right-top {
  padding-top: 10rpx;
  font-size: 17px;
  line-height: 1.3;
  height: 44px;
  display: -webkit-box;
  -webkit-box-pack: center;
}
.song .head .head-right .right-bottom {
  padding-top: 1px;
  font-size: 28rpx;
  height: 60rpx;
  display: flex;
  align-items: center;
  overflow: hidden;
}
.song .head .head-right .right-bottom .img6 {
  height: 100%;
  width: 60rpx;
  border-radius: 50%;
}
.song .head .head-right .right-bottom .text {
  line-height: 60rpx;
  height: 60rpx;
  margin-left: 14rpx;
}
.song .song-list {
  height: 46rpx;
  width: 97%;
  padding-left: 3%;
  font-size: 22rpx;
  line-height: 46rpx;
  background-color: rgba(231, 230, 232, 0.5);
}
.song .active {
  display: none;
}
.song .recommend-text1 {
  padding: 0rpx 18rpx;
}
.song .recommend-text1 .newests {
  color: #dd001b;
  width: 100%;
  height: 100rpx;
  margin-bottom: 10rpx;
  position: relative;
}
.song .recommend-text1 .newests .newests-left {
  float: left;
  color: #888888;
  width: 7%;
  text-align: center;
  margin-right: 3%;
  height: 100%;
  line-height: 100rpx;
  font-size: 40rpx;
}
.song .recommend-text1 .newests .newests-right .authors {
  float: left;
  font-size: 34rpx;
  color: #333;
  height: 60rpx;
  line-height: 60rpx;
  width: 82%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.song .recommend-text1 .newests .newests-right .SongTitles {
  float: left;
  width: 80%;
  height: 40rpx;
  font-size: 24rpx;
  color: #888888;
}
.song .recommend-text1 .newests .newests-right .SongTitles .img1 {
  width: 10%;
  float: left;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-position-y: 5rpx;
  background-size: 850%,850%;
  background-position-x: 4rpx;
}
.song .recommend-text1 .newests .newests-right .SongTitles .left {
  width: 90%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break: normal;
  float: left;
}
.song .recommend-text1 .newests .img2 {
  position: absolute;
  right: 0;
  top: 0;
  width: 7%;
  height: 100rpx;
  overflow: hidden;
  background-image: url(data:image/png;base64,);
  background-position-x: -54rpx;
  background-position-y: 28rpx;
  background-size: 750%,750%;
}
.song .jia {
  display: flex;
  justify-content: center;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
}
.song .jia .m1 {
  width: 0px;
  height: 0px;
  border: 20rpx solid;
  border-color: red yellow green blue;
  border-color: transparent transparent green transparent;
}
.song .jia .m2 {
  width: 0px;
  border: 20rpx solid;
  margin-top: 20rpx;
  border-color: green yellow red blue;
  border-color: green transparent transparent transparent;
}
.song .comment {
  height: 46rpx;
  width: 97%;
  padding-left: 3%;
  font-size: 22rpx;
  line-height: 46rpx;
  background-color: rgba(231, 230, 232, 0.5);
}
.song .da {
  width: 30rpx;
  height: 30rpx;
}
.song::before {
  content: '';
  position: absolute;
  top: 0;
  height: 330rpx;
  width: 100%;
  background: inherit;
  background-image: linear-gradient(to right, #000a00, rgba(0, 10, 0, 0.5), black);
  background-attachment: fixed;
  -webkit-filter: blur(4px);
  -moz-filter: blur(4px);
  -ms-filter: blur(4px);
  filter: blur(4px);
}

